cp freedup.spec ~/rpmbuild/SPEC
cd ../SOURCES
wget http://www.freedup.org/freedup-1.6-3-src.tar.bz2
cd ../SPECS

edit spec file, adjust version and release and changelogs
only for freedup-1.6-3, open the tar.bz2 and change the folder name freedup-1.6-3 to freedup-1.6

rpmbuild -bs --nodeps freedup.spec 
mockbuild /home/build/rpmbuild/SRPMS/freedup-1.6-3.src.rpm  smeserver-9-i386-base
mockbuild /home/build/rpmbuild/SRPMS/freedup-1.6-3.src.rpm  smeserver-9-x86_64-base

